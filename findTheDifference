class Solution {
    public char findTheDifference(String s, String t) {
        
           char[] charsS = s.toCharArray();
    char[] charsT = t.toCharArray();
        
            Arrays.sort(charsS);
            Arrays.sort(charsT);
        
        
        for(int i=0;i<charsS.length;i++){
            
            if(charsS[i]!=charsT[i]){
                return charsT[i];
            }
        }

 return charsT[charsT.length-1];

                }
                
                }
